
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()
local Window = Library.CreateLib("Smilley", "RJTheme3")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local i = 0
local s = Instance.new("Highlight")

local Tab = Window:NewTab("Main")

-- Подсекция
local Section = Tab:NewSection("Functions")


Section:NewSlider("Speed Hack", "Adding Speed To Your Character", 200, 0, function(s) -- 200 (Макс. значение) | 0 (Мин. значение)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewButton("Baldy Ultra", "Can work on other characters (after died - walk speed removing)", function()
player.Character:FindFirstChildOfClass("Humanoid").Name = "Humanoid"
while wait(0.0000000000000000000000000001) do
player.Character.Humanoid.WalkSpeed = 100
end
end)

Section:NewButton("Baldy Ultra OFF", "Can work on other characters (after died - walk speed removing)", function()
player.Character.Humanoid.Name = "HumanoidE"
end)

Section:NewButton("Inf Stamina", "Can work on other characters (after died - walk speed removing)", function()
player.Character.Values.Stamina.Value = 999999999
end)

Section:NewButton("Remove Dark", "Can work on other characters (after died - walk speed removing)", function()
if not _G.FullBrightExecuted then

	_G.FullBrightEnabled = false

	_G.NormalLightingSettings = {
		Brightness = game:GetService("Lighting").Brightness,
		ClockTime = game:GetService("Lighting").ClockTime,
		FogEnd = game:GetService("Lighting").FogEnd,
		GlobalShadows = game:GetService("Lighting").GlobalShadows,
		Ambient = game:GetService("Lighting").Ambient
	}

	game:GetService("Lighting"):GetPropertyChangedSignal("Brightness"):Connect(function()
		if game:GetService("Lighting").Brightness ~= 1 and game:GetService("Lighting").Brightness ~= _G.NormalLightingSettings.Brightness then
			_G.NormalLightingSettings.Brightness = game:GetService("Lighting").Brightness
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").Brightness = 1
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("ClockTime"):Connect(function()
		if game:GetService("Lighting").ClockTime ~= 12 and game:GetService("Lighting").ClockTime ~= _G.NormalLightingSettings.ClockTime then
			_G.NormalLightingSettings.ClockTime = game:GetService("Lighting").ClockTime
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").ClockTime = 12
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("FogEnd"):Connect(function()
		if game:GetService("Lighting").FogEnd ~= 786543 and game:GetService("Lighting").FogEnd ~= _G.NormalLightingSettings.FogEnd then
			_G.NormalLightingSettings.FogEnd = game:GetService("Lighting").FogEnd
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").FogEnd = 786543
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("GlobalShadows"):Connect(function()
		if game:GetService("Lighting").GlobalShadows ~= false and game:GetService("Lighting").GlobalShadows ~= _G.NormalLightingSettings.GlobalShadows then
			_G.NormalLightingSettings.GlobalShadows = game:GetService("Lighting").GlobalShadows
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").GlobalShadows = false
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("Ambient"):Connect(function()
		if game:GetService("Lighting").Ambient ~= Color3.fromRGB(178, 178, 178) and game:GetService("Lighting").Ambient ~= _G.NormalLightingSettings.Ambient then
			_G.NormalLightingSettings.Ambient = game:GetService("Lighting").Ambient
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)
		end
	end)

	game:GetService("Lighting").Brightness = 1
	game:GetService("Lighting").ClockTime = 12
	game:GetService("Lighting").FogEnd = 786543
	game:GetService("Lighting").GlobalShadows = false
	game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)

	local LatestValue = true
	spawn(function()
		repeat
			wait()
		until _G.FullBrightEnabled
		while wait() do
			if _G.FullBrightEnabled ~= LatestValue then
				if not _G.FullBrightEnabled then
					game:GetService("Lighting").Brightness = _G.NormalLightingSettings.Brightness
					game:GetService("Lighting").ClockTime = _G.NormalLightingSettings.ClockTime
					game:GetService("Lighting").FogEnd = _G.NormalLightingSettings.FogEnd
					game:GetService("Lighting").GlobalShadows = _G.NormalLightingSettings.GlobalShadows
					game:GetService("Lighting").Ambient = _G.NormalLightingSettings.Ambient
				else
					game:GetService("Lighting").Brightness = 1
					game:GetService("Lighting").ClockTime = 12
					game:GetService("Lighting").FogEnd = 786543
					game:GetService("Lighting").GlobalShadows = false
					game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)
				end
				LatestValue = not LatestValue
			end
		end
	end)
end
_G.FullBrightExecuted = true
_G.FullBrightEnabled = not _G.FullBrightEnabled
end)


local Tab = Window:NewTab("Esp")

-- Подсекция
local Section = Tab:NewSection("Functions")
Section:NewButton("Esp Baldy", "Esp", function()
for i , obj in pairs(game.Workspace.Game.Players:GetChildren()) do
if obj.Name == "Baldi" then
if not obj:FindFirstChild("Highlight") then
local ss = s:Clone()
ss.FillColor = Color3.fromRGB(255,0,0)
ss.OutlineColor = Color3.fromRGB(0,0,0)
ss.Parent = obj
end
end
end
end)

Section:NewButton("Esp players", "Esp", function()
for i , obj in pairs(game.Workspace.Game.Players:GetChildren()) do
if obj.Name ~= "Baldi" then
if not obj:FindFirstChild("Highlight") then
local ss = s:Clone()
ss.OutlineColor = Color3.fromRGB(0,0,0)
ss.FillColor = Color3.fromRGB(0,255,0)
ss.Parent = obj
end
end
end
end)

Section:NewButton("Esp Notebooks", "Esp", function()
for i, obj in pairs(game.Workspace.Game.Map.Notebooks:GetDescendants()) do
	if not obj:FindFirstChild("Highlight") then
	if obj.Name == "Notebook" then
    local ss = s:Clone()
	ss.OutlineColor = Color3.fromRGB(0,0,0)
    ss.FillColor = Color3.fromRGB(0, 0, 255)
    ss.Parent = obj
	end
	end
end
end)

Section:NewButton("Esp Items", "Esp", function()
local Map = game.Workspace.Game:FindFirstChild("Map")
if Map then
    local Container = Map:FindFirstChild("Item") or Map:FindFirstChild("Items")
    if Container then
        for _, obj in pairs(Container:GetChildren()) do
            if not obj:FindFirstChild("Highlight") then
                local ss = s:Clone()
                ss.OutlineColor = Color3.fromRGB(0, 0, 0)
                ss.FillColor = Color3.fromRGB(125, 125, 0)
                ss.Name = "Highlight"
                ss.Parent = obj
            end
        end
    end
end
end)


Section:NewButton("Esp Levers", "Esp", function()
local Map = game.Workspace.Game:FindFirstChild("Map")
if Map then
    local Container = Map:FindFirstChild("Levers")
    if Container then
        for _, obj in pairs(Container:GetChildren()) do
            if not obj:FindFirstChild("Highlight") then
                local ss = s:Clone()
                ss.OutlineColor = Color3.fromRGB(0, 0, 0)
                ss.FillColor = Color3.fromRGB(255, 255, 255)
                ss.Name = "Highlight"
                ss.Parent = obj
            end
        end
    end
end
end)

Section:NewButton("Esp Vendings", "Esp", function()
local Map = game.Workspace.Game:FindFirstChild("Map")
if Map then
    local Container = Map:FindFirstChild("Vendings")
    if Container then
        for _, obj in pairs(Container:GetChildren()) do
            if not obj:FindFirstChild("Highlight") then
				if obj.Name == "BsodaMachine" then
                local ss = s:Clone()
                ss.OutlineColor = Color3.fromRGB(0, 0, 0)
                ss.FillColor = Color3.fromRGB(0, 0, 255)
                ss.Name = "Highlight"
                ss.Parent = obj
				elseif obj.Name == "ZestyMachine" then
				local ss = s:Clone()
                ss.OutlineColor = Color3.fromRGB(0, 0, 0)
                ss.FillColor = Color3.fromRGB(255,165,0)
                ss.Name = "Highlight"
                ss.Parent = obj
				elseif obj.Name == "DietSodaMachine" then
                local ss = s:Clone()
                ss.OutlineColor = Color3.fromRGB(0, 0, 0)
                ss.FillColor = Color3.fromRGB(0, 255, 255)
                ss.Name = "Highlight"
                ss.Parent = obj
				end
            end
        end
    end
end
end)

local Tab = Window:NewTab("Teleport")

-- Подсекция
local Section = Tab:NewSection("Functions")

Section:NewDropdown("Teleport", "If dont work restart", {game.Workspace.Game.Players:GetChildren()[1].Name, game.Workspace.Game.Players:GetChildren()[2].Name, game.Workspace.Game.Players:GetChildren()[3].Name , game.Workspace.Game.Players:GetChildren()[4].Name
, game.Workspace.Game.Players:GetChildren()[5].Name}, function(s)
    if s == game.Workspace.Game.Players:GetChildren()[1].Name then
    player.Character:PivotTo(game.Workspace.Game.Players:GetChildren()[1].HumanoidRootPart.CFrame)
    elseif s == game.Workspace.Game.Players:GetChildren()[2].Name then
    player.Character:PivotTo(game.Workspace.Game.Players:GetChildren()[2].HumanoidRootPart.CFrame)
    elseif s == game.Workspace.Game.Players:GetChildren()[3].Name then
    player.Character:PivotTo(game.Workspace.Game.Players:GetChildren()[3].HumanoidRootPart.CFrame)
    elseif s == game.Workspace.Game.Players:GetChildren()[4].Name then
    player.Character:PivotTo(game.Workspace.Game.Players:GetChildren()[4].HumanoidRootPart.CFrame)
	elseif s == game.Workspace.Game.Players:GetChildren()[5].Name then
    player.Character:PivotTo(game.Workspace.Game.Players:GetChildren()[5].HumanoidRootPart.CFrame)
    end
end)
